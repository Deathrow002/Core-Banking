{
  "info": {
    "name": "Core Bank Postman Collection",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Authentication Service",
      "item": [
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "url": {
              "raw": "http://localhost:8080/api/v1/auth/login?email={{email}}&password={{password}}",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8080",
              "path": ["api", "v1", "auth", "login"],
              "query": [
                { "key": "email", "value": "{{email}}" },
                { "key": "password", "value": "{{password}}" }
              ]
            }
          }
        },
        {
          "name": "Register",
          "request": {
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"test@example.com\",\n  \"password\": \"password\",\n  ...\n}"
            },
            "url": {
              "raw": "http://localhost:8080/api/v1/auth/register",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8080",
              "path": ["api", "v1", "auth", "register"]
            }
          }
        },
        {
          "name": "Get All Users",
          "request": {
            "method": "GET",
            "url": {
              "raw": "http://localhost:8080/api/v1/auth/getAllUsers",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8080",
              "path": ["api", "v1", "auth", "getAllUsers"]
            }
          }
        }
      ]
    },
    {
      "name": "Customer Service",
      "item": [
        {
          "name": "Create Customer",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"firstName\": \"John\",\n  \"lastName\": \"Doe\",\n  \"email\": \"john.doe@example.com\",\n  \"phoneNumber\": \"1234567890\",\n  \"nationalId\": \"123456789\",\n  \"dateOfBirth\": \"1990-01-01\",\n  \"status\": \"Active\",\n  \"addresses\": [\n    {\n      \"address\": \"123 Main St\",\n      \"subDistrict\": \"Downtown\",\n      \"district\": \"Central\",\n      \"city\": \"Springfield\",\n      \"country\": \"USA\",\n      \"zipCode\": \"62701\",\n      \"addressType\": \"Present\"\n    }\n  ]\n}"
            },
            "url": {
              "raw": "http://localhost:8083/customers",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8083",
              "path": ["customers"]
            }
          }
        },
        {
          "name": "Get Customer by ID",
          "request": {
            "method": "GET",
            "url": {
              "raw": "http://localhost:8083/customers?customerId={{customerId}}",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8083",
              "path": ["customers"],
              "query": [
                {
                  "key": "customerId",
                  "value": "{{customerId}}"
                }
              ]
            }
          }
        },
        {
          "name": "Get All Customers",
          "request": {
            "method": "GET",
            "url": {
              "raw": "http://localhost:8083/customers/all",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8083",
              "path": ["customers", "all"]
            }
          }
        },
        {
          "name": "Update Customer",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"firstName\": \"Jane\",\n  \"lastName\": \"Doe\",\n  \"email\": \"jane.doe@example.com\",\n  \"phoneNumber\": \"0987654321\",\n  \"nationalId\": \"987654321\",\n  \"dateOfBirth\": \"1992-02-02\",\n  \"status\": \"Inactive\",\n  \"addresses\": [\n    {\n      \"address\": \"456 Elm St\",\n      \"subDistrict\": \"Uptown\",\n      \"district\": \"North\",\n      \"city\": \"Springfield\",\n      \"country\": \"USA\",\n      \"zipCode\": \"62702\",\n      \"addressType\": \"WorkPlace\"\n    }\n  ]\n}"
            },
            "url": {
              "raw": "http://localhost:8083/customers?customerId={{customerId}}",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8083",
              "path": ["customers"],
              "query": [
                {
                  "key": "customerId",
                  "value": "{{customerId}}"
                }
              ]
            }
          }
        },
        {
          "name": "Validate Customer By ID",
          "request": {
            "method": "GET",
            "url": {
              "raw": "http://localhost:8083/customers/validateById?customerId={{customerId}}",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8083",
              "path": ["customers", "validateById"],
              "query": [
                { "key": "customerId", "value": "{{customerId}}" }
              ]
            }
          }
        },
        {
          "name": "Validate Customer By Data",
          "request": {
            "method": "GET",
            "url": {
              "raw": "http://localhost:8083/customers/validateByData?customerId={{customerId}}&email={{email}}",
              "protocol": "http",
              "host": ["localhost"],
              "port": "8083",
              "path": ["customers", "validateByData"],
              "query": [
                { "key": "customerId", "value": "{{customerId}}" },
                { "key": "email", "value": "{{email}}" }
              ]
            }
          }
        }
      ]
    },
